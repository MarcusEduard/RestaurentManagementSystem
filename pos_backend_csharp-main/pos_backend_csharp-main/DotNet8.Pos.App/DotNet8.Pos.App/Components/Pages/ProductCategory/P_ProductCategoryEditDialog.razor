<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="@reqModel.ProductCategoryCode" ReadOnly="true" Label="ProductCategory Code" />
        <MudTextField @bind-Value="@reqModel.ProductCategoryName" Label="Product Category Name" />              
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Success" OnClick="SaveAsync">Save</MudButton>
    </DialogActions>
</MudDialog>
